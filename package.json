{
  "name": "swedishid",
  "version": "0.1.0",
  "description": "Validate, parse, format, and extract metadata from Swedish personnummer and organisationsnummer",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist", "README.md"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": ["swedish", "personnummer", "organisationsnummer", "validation", "sweden", "identity"],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/harr3n/swedishid.git"
  },
  "homepage": "https://github.com/harr3n/swedishid#readme",
  "bugs": {
    "url": "https://github.com/harr3n/swedishid/issues"
  },
  "packageManager": "pnpm@9.14.2",
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^4.0.18"
  }
}
